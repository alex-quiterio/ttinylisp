{
	"version": "0.2.0",
	"configurations": [
		{
      "name": "Launch Tests",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath} test",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}", "--extensionTestsPath=${workspaceRoot}/out/test" ],
      "stopOnEntry": false,
      "sourceMaps": true,
      "outDir": "out",
      "preLaunchTask": "npm"
    }
	]
}